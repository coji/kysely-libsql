{
  "name": "@coji/kysely-libsql",
  "version": "0.2.0",
  "description": "kysely diarect for libsql",
  "repository": {
    "type": "git",
    "url": "github:coji/kysely-libsql"
  },
  "homepage": "https://github.com/coji/kysely-libsql",
  "authors": [
    "MIZOGUCHI Coji <coji@techtalk.jp>"
  ],
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "vite build",
    "format": "prettier --cache -c .",
    "lint": "biome lint .",
    "test": "vitest --run",
    "typecheck": "tsc --noEmit",
    "validate": "run-p test lint format typecheck"
  },
  "keywords": [
    "libsql",
    "turso",
    "kysely",
    "database"
  ],
  "devDependencies": {
    "@biomejs/biome": "^1.6.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "typescript": "^5.4.2",
    "vite": "^5.1.6",
    "vite-plugin-dts": "^3.7.3",
    "vitest": "^1.4.0"
  },
  "peerDependencies": {
    "@libsql/client": "*",
    "kysely": "*"
  }
}
